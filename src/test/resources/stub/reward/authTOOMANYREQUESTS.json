{
  "request": {
    "method": "POST",
    "urlPathPattern": "/reward/INITIATIVEID",
    "bodyPatterns": [
      {
        "equalToJson": {
          "transactionId": "${json-unit.any-string}",
          "correlationId":  "${json-unit.any-string}",
          "userId": "USERID",
          "merchantId": "MERCHANTID",
          "senderCode": "${json-unit.regex}SENDERCODE[0-9]+",
          "merchantFiscalCode":  "${json-unit.regex}MERCHANTFISCALCODE[0-9]+",
          "vat": "TOOMANYREQUESTS",
          "trxDate": "${json-unit.regex}[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?\\+[0-9]{2}:[0-9]{2}",
          "trxChargeDate": "${json-unit.regex}[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?\\+[0-9]{2}:[0-9]{2}",
          "amountCents":  "${json-unit.any-number}",
          "amountCurrency": "${json-unit.regex}AMOUNTCURRENCY[0-9]+",
          "mcc":  "${json-unit.regex}MCC[0-9]+",
          "acquirerCode": "${json-unit.regex}ACQUIRERCODE[0-9]+",
          "acquirerId":  "${json-unit.regex}ACQUIRERID[0-9]+",
          "idTrxIssuer":  "${json-unit.regex}IDTRXISSUER[0-9]+",
          "idTrxAcquirer": "${json-unit.regex}IDTRXACQUIRER[0-9]+",
          "operationType": "CHARGE"
        }
      }
    ]
  },
  "response": {
    "status": "429"
  }
}