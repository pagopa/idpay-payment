{
  "request": {
    "method": "GET",
    "urlPathPattern": "/idpay/merchant/portal/initiatives/(INITIATIVEID|BARCODE_INITIATIVEID_REWARDED|BARCODE_INITIATIVEID_TOOMANYREQUEST)",
    "headers": {
        "x-merchant-id": {
          "equalTo": "MERCHANTID"
        }
    }
  },
  "response": {
    "status": "200",
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"],
    "jsonBody": {
      "initiativeId": "INITIATIVEID",
      "initiativeName": "INITIATIVENAME",
      "businessName": "BUSINESSNAME",
      "legalOfficeAddress": "LEGALOFFICEADDRESS",
      "legalOfficeMunicipality": "LEGALOFFICEMUNICIPALITY",
      "legalOfficeProvince": "LEGALOFFICEPROVICE",
      "legalOfficeZipCode": "LEGALOFFICEZUPCODE",
      "certifiedEmail": "CERTIFIEDEMAIL",
      "fiscalCode": "MERCHANTFISCALCODE0",
      "vatNumber": "VAT0",
      "status": "STATUS",
      "iban": "IBAN"
    }
  }
}